<?xml version="1.0" encoding="utf-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>Serenity.Web.Assets</id>
    <version>3.14.4</version>
    <authors>${authors}</authors>
    <owners>${owners}</owners>
    <title>Serenity Platform - Web Assets</title>
    <description>Contains static assets for Serenity web applications</description>
    <language>${language}</language>
    <iconUrl>${iconUrl}</iconUrl>
    <license type="expression">MIT</license>
    <projectUrl>${projectUrl}</projectUrl>
    <copyright>${copyright}</copyright>
    <tags>${tags}</tags>
    <dependencies>
      <dependency id="jQuery" version="${net461:jQuery}" />
      <dependency id="jQuery.UI.Combined" version="${net45:jQuery.UI.Combined}" />
      <dependency id="jQuery.Validation" version="${net461:jQuery.Validation}" />
      <dependency id="bootstrap" version="${net452:bootstrap}" />
      <dependency id="toastr" version="${net45:toastr}" />
      <dependency id="Select2.js" version="[3.5.1]" />
    </dependencies>
  </metadata>
  <files>
    <file src="Assets\Content\**\*" exclude="**\*.bak;**\*.log;**\*.orig" target="content\Content" />
    <file src="Assets\fonts\**\*" exclude="**\*.bak;**\*.log;**\*.orig" target="content\fonts" />
    <file src="Assets\Scripts\**\*" exclude="**\*.bak;**\*.log;**\*.orig" target="content\Scripts" />
    <file src="Style\images\*.png" target="content/Content/serenity/images" />
    <file src="Style\images\*.gif" target="content/Content/serenity/images" />
    <file src="Style\images\*.jpg" target="content/Content/serenity/images" />
  </files>
</package>