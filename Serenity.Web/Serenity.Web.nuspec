      <group targetFramework=".NETStandard2.0">
        <dependency id="Dapper" version="${netstandard2.0:Dapper}" />
        <dependency id="Microsoft.AspNetCore.Mvc.Core" version="${netstandard2.0:Microsoft.AspNetCore.Mvc.Core}" />
