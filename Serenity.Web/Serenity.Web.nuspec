<?xml version="1.0" encoding="utf-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>${id}</id>
    <version>${version}</version>
    <authors>${authors}</authors>
    <owners>${owners}</owners>
    <title>${title}</title>
    <description>${description}</description>
    <language>${language}</language>
    <iconUrl>${iconUrl}</iconUrl>
    <licenseUrl>${licenceUrl}</licenseUrl>
    <projectUrl>${projectUrl}</projectUrl>
    <copyright>${copyright}</copyright>
    <tags>${tags}</tags>
    <dependencies>
      <dependency id="Newtonsoft.Json" version="${jsonNetVersion}" />
      <dependency id="Microsoft.AspNet.Mvc" version="${microsoftAspNetMvcVersion}" />
      <dependency id="Microsoft.AspNet.Razor" version="${microsoftAspNetRazorVersion}" />
      <dependency id="Microsoft.AspNet.WebPages" version="${microsoftAspNetWebPagesVersion}" />
      <dependency id="Microsoft.Web.Infrastructure" version="${microsoftWebInfrastructureVersion}" />
      <dependency id="MsieJavaScriptEngine" version="1.7.0" />
      <dependency id="Serenity.Core" version="${version}" />
      <dependency id="Serenity.Data" version="${version}" />
      <dependency id="Serenity.Data.Entity" version="${version}" />
      <dependency id="Serenity.Services" version="${version}" />
      <dependency id="jQuery" version="2.2.0" />
      <dependency id="jquery.blockUI.TypeScript.DefinitelyTyped" version="0.0.4" />
      <dependency id="jquery.cookie.TypeScript.DefinitelyTyped" version="0.2.1" />
      <dependency id="jquery.event.drag" version="2.2.0" />
      <dependency id="jQuery.UI.Combined" version="1.11.4" />
      <dependency id="jQuery.Validation" version="1.14.0" />
      <dependency id="bootstrap" version="3.3.6" />
      <dependency id="toastr" version="2.1.1" />
      <dependency id="Select2.js" version="[3.5.1]" />
      <dependency id="jquery.TypeScript.DefinitelyTyped" version="3.0.5" />
      <dependency id="jqueryui.TypeScript.DefinitelyTyped" version="1.4.6" />
      <dependency id="jquery.validation.TypeScript.DefinitelyTyped" version="0.2.8" />
      <dependency id="sortablejs.TypeScript.DefinitelyTyped" version="0.1.7" />
      <dependency id="toastr.TypeScript.DefinitelyTyped" version="0.3.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="${id}.dll" target="lib/${framework}" />
    <file src="${id}.pdb" target="lib/${framework}" />
    <file src="..\..\Includes\web.config.transform" target="content\web.config.transform" />
    <file src="..\..\Includes\Content\**\*" exclude="**\*.bak;**\*.log;**\*.orig" target="content\Content" />
    <file src="..\..\Includes\Fonts\**\*" exclude="**\*.bak;**\*.log;**\*.orig" target="content\fonts" />
    <file src="..\..\Includes\Scripts\**\*" exclude="**\*.bak;**\*.log;**\*.orig"  target="content\Scripts" />
    <file src="serenity.css" target="content/Content/serenity" />
    <file src="..\..\Style\images\*.png" target="content/Content/serenity/images" />
    <file src="..\..\Style\images\*.gif" target="content/Content/serenity/images" />
    <file src="..\..\Style\images\*.jpg" target="content/Content/serenity/images" />
    <file src="..\..\Scripts\typings\rsvp\rsvp.d.ts" target="content/Scripts/typings/rsvp" />
    <file src="..\..\Scripts\typings\slickgrid\slickgrid.d.ts" target="content/Scripts/typings/slickgrid" />
    <file src="..\..\Scripts\Serenity\Serenity.CoreLib.js" target="content/Scripts/serenity" />
    <file src="..\..\Scripts\Serenity\Serenity.CoreLib.min.js" target="content/Scripts/serenity" />
    <file src="..\..\Scripts\Serenity\Serenity.CoreLib.d.ts" target="content/Scripts/serenity" />
    <file src="..\..\..\Serenity.Script.UI\bin\${configuration}\Serenity.Script.UI.js" target="content/Scripts/serenity" />
    <file src="..\..\..\Serenity.Script.UI\bin\${configuration}\Serenity.Script.UI.min.js" target="content/Scripts/serenity" />
  </files>
</package>